{
    "name": "understand/understand-laravel5",
    "description": "Laravel 5/6 service provider for Understand.io",
    "keywords": ["laravel", "understand.io", "understand", "logs", "laravel 5", "Laravel 6"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aivis",
            "email": "aivis.silins@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-curl": "*",
        "illuminate/support": "~5.0|~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "^3.5",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
		"psr-0": {
            "Understand\\UnderstandLaravel5\\": "src"
		}
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 5", "Laravel 6"],
        "branch-alias": {
			"dev-master": "2.0-dev"
		},
        "laravel": {
            "providers": [
                "Understand\\UnderstandLaravel5\\UnderstandLaravel5ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
