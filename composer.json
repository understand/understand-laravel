{
    "name": "understand/understand-laravel",
    "description": "Laravel 5, 6 and 7 service provider for Understand.io",
    "keywords": ["laravel", "understand.io", "understand", "logs", "laravel 5", "Laravel 6"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aivis",
            "email": "aivis.silins@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5|^7.0|^8.0",
        "ext-curl": "*",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0",
        "orchestra/testbench": "3.*|4.*|5.*|6.*",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
		"psr-0": {
            "Understand\\UnderstandLaravel5\\": "src"
		}
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 5", "Laravel 6", "Laravel 7", "Laravel 8"],
        "branch-alias": {
			"dev-master": "2.0-dev"
		},
        "laravel": {
            "providers": [
                "Understand\\UnderstandLaravel5\\UnderstandLaravel5ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
